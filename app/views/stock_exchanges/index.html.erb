<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", stock_exchanges_path  %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title">Stock Exchanges</h2>
    <div class="inner">
      <table class="table">
        <% @stock_exchanges.each do |stock_exchange| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= link_to stock_exchange.name, stock_exchanges_path(stock_exchange) %>
          </td>
          <td class="last">
            
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>

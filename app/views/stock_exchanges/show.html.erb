<div class="block">          
  <h2 class="title">Stock Exchanges</h2>
  <table class="table">
    <thead>
      <tr></tr>
      <tr>
        <th>Stock</th>
        <th>Sector</th>
      </tr>
    </thead>
    <% @exchange_listings.each do |stock| -%>
      <tr class="<%= cycle("odd", "even") %>">
        <td><%= stock.name %></td>
        <td><%= stock.category_id %></td>
        <td class="last">    
        </td>
      </tr>          
    <% end -%>
  </table>
</div>
